
@{
    ViewBag.Title = "PuntoAtencion";
    Layout = ViewBag.Layout;
}

<div class="container" style="margin-top: 15px; margin-bottom: 15px">
    <div class="card" style="margin-top: 15px">
        <h5 class="card-header">Puntos de Atención</h5>
        <div class="card-body">
            <div class="row">
                <div class="col-md-6 d-grid gap-2 d-flex justify-content-end">
                   
                </div>
                <div class="col-md-6 d-grid gap-2 d-flex justify-content-end">
                    <a class="btn btn-outline-dark"
                       href='@Url.Action("Bienvenida", "MenuPrincipal")'>
                        Agregar
                        <i class="fa fa-plus-circle"></i>
                    </a>
                </div>
            </div>
            <br/>
            <div class="row">
                <div class="col-md-6">
                    @Html.DropDownList("regiones", (List<SelectListItem>)ViewBag.RegionesSelectList, "Seleccionar región")
                </div>
                <div class="col-md-6 d-grid gap-2 d-flex justify-content-end">
                    <div class="input-group mb-3">
                        <span class="input-group-text bg-dark text-white">Fecha</span>
                        <input type="text" id="" class="form-control" />
                    </div>
                </div>
            </div>
        </div>
        <div class="card-footer">
            <p class="h10">Administración de puntos de atención</p>
        </div>
    </div>
</div>

@section Scripts{

}